	</listeners>
	<test name="Executing in chrome">
		<parameter name="browser" value="chrome"/>
		<classes>
			<class name="com.sevenrmartsupermarket.tests.HomeTest">

			</class>

			<class name="com.sevenrmartsupermarket.tests.LoginTest">


			</class>
