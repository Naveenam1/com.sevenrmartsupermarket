			</class>

			<class name="com.sevenrmartsupermarket.tests.LoginTest">


			</class>
			
			
		</classes>
	</test>


	<test name="Executing in firefox">
		<parameter name="browser" value="firefox"/>
		<classes>
			<class name="com.sevenrmartsupermarket.tests.HomeTest">

			</class>

			<class name="com.sevenrmartsupermarket.tests.LoginTest">


			</class>
		</classes>
